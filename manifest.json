{
    "manifest_version": 2,
    
    "name": "Encrypted Password",
    "description": "This extension generates encrypted passwords to any page that you want",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },

    "icons": {
      "128": "icon.png",
      "64":  "icon.png",
      "32":  "icon.png",
      "16":  "icon.png"    
    },
    
    "permissions": [
      "contextMenus"
    ],
    
    "content_scripts": [{
        "matches": ["http://*/*","https://*/*"],
        "js": ["content_script.js"]
    }]
}
